{
  "name": "@supermodel/lib",
  "version": "0.4.10",
  "description": "Supermodel.io core functionality library",
  "main": "lib/index.js",
  "scripts": {
    "build:clean": "rm -r ./lib",
    "build": "npm run build:clean && tsc",
    "lint": "tslint --format stylish --project ./",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test",
    "pretest": "npm run ts && npm run lint",
    "test": "npm run unit",
    "test:watch": "jest --watch",
    "ts": "tsc --noEmit",
    "unit": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./scripts/prettify-staged.sh",
      "post-commit": "git update-index -g"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/supermodel/superlib.git"
  },
  "author": "Zdenek Nemec <z@goodapi.co> (http://goodapi.co)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/supermodel/superlib/issues"
  },
  "homepage": "https://github.com/supermodel/superlib#readme",
  "devDependencies": {
    "@supermodel/file": "0.5.0",
    "@types/jest": "23.3.10",
    "@types/json-schema": "7.0.1",
    "@types/node": "10.12.18",
    "husky": "1.2.1",
    "jest": "^23.4.0",
    "prettier": "1.15.3",
    "ts-jest": "23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.2.2"
  },
  "dependencies": {
    "ajv": "^6.4.0",
    "casex": "0.0.5",
    "graphql": "0.13.2",
    "js-yaml": "^3.11.0",
    "jsonpointer": "4.0.1",
    "url": "0.11.0"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
